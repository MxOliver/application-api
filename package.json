{
  "name": "application-api",
  "version": "1.0.0",
  "description": "The GraphQL API for the High Fives Application Process",
  "main": "index.js",
  "scripts": {
    "deploy": "serverless deploy",
    "format": "npm run format:prettier",
    "format:prettier": "node_modules/.bin/prettier --config prettier.config.js \"src/**/*.{ts,css,less,js}\" --write",
    "format-test": "npm run format-test:prettier",
    "format-test:prettier": "node_modules/.bin/prettier --config prettier.config.js \"src/**/*.{ts,css,less,js}\" -l"
  },
  "husky": {
    "hooks": {
      "pre-commit": "node_modules/.bin/lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,css,less,js}": [
      "prettier --config prettier.config.js -l"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HighFivesFoundation/application-api.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/HighFivesFoundation/application-api/issues"
  },
  "homepage": "https://github.com/HighFivesFoundation/application-api#readme",
  "dependencies": {
    "apollo-server-lambda": "^2.4.6",
    "aws-sdk": "^2.409.0",
    "graphql": "^14.1.1",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "babel-loader": "^8.0.5",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.4",
    "prettier": "^1.16.4",
    "serverless": "^1.38.0",
    "serverless-offline": "^4.6.0",
    "serverless-webpack": "^5.2.0",
    "webpack": "^4.29.5",
    "webpack-graphql-loader": "^1.0.0",
    "webpack-node-externals": "^1.7.2"
  }
}
